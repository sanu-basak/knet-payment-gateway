{
    "name": "o2one/knet_payment_gateway",
    "description": "This package is provides an expressive, fluent interface to Knet's payment services.",
    "require": {
        "php": "^7.3",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.9",
        "orchestra/testbench": "^6.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sanu",
            "email": "sanu@o2onelabs.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "O2oneLabs\\KnetPaymentGateway\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "O2oneLabs\\KnetPaymentGateway\\KnetServiceProvider"
            ]
        }
    }
}